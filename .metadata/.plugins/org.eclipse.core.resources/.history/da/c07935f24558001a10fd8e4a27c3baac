package java_oo;
//面向对象2
//封装 ：该隐藏的隐藏起来，该暴露的暴露出来。
public class OO_Test {
	public static void main(String[] args) {
		Animal a = new Animal();
		a.name = "pig";
		a.age = 3;
		a.legs = 4;
		a.eat();
	}
}


class Animal{
	String name;
	int age;
	int legs;
	
	public void eat() {
		System.out.println("eat");
	}
	
}