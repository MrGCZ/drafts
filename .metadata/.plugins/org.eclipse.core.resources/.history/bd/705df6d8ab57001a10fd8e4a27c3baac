package java_bb;

public class ArrayTest1 {
	public static void main(String[] args) {
		int[] array1 = new int[] {1,2,3,4,5};
		printarray(array1);
		int[] array2;
		array2 = array1;
		array2[0] = 100;
		printarray(array2);
	}
	
	public static void printarray(int[] l_array) {
		for(int i=0;i<l_array.length;i++) {
			System.out.println(l_array[i] + "\t");			
		}
	}
	
}
